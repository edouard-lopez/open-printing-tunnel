{
  "name": "frontend",
  "author": "Edouard Lopez <edouard@akema.fr>",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "npm install && webpack-dev-server --inline --hot --host 0.0.0.0",
    "build": "rm -rf dist/ && NODE_ENV=production webpack --progress --hide-modules",
    "test": "ava test && xo",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "ava": "0.18.x",
    "axios": "0.21.x",
    "babel-core": "6.23.x",
    "babel-loader": "6.3.x",
    "babel-plugin-transform-runtime": "6.23.x",
    "babel-preset-es2015": "6.22.x",
    "babel-preset-stage-2": "6.22.x",
    "babel-runtime": "6.23.x",
    "bootstrap": "4.0.0-alpha.4",
    "css-loader": "0.26.x",
    "eslint": "3.x.x",
    "eslint-config-angular": "0.5.x",
    "eslint-plugin-angular": "1.6.x",
    "file-loader": "0.10.x",
    "font-awesome": "4.7.x",
    "hint.css": "2.4.x",
    "husky": "0.13.x",
    "jquery": "3.1.x",
    "json-loader": "0.5.x",
    "moment": "2.17.x",
    "nock": "^8.0.0",
    "node-localstorage": "^1.3.0",
    "node-sass": "3.7.x",
    "notie": "3.9.x",
    "pilou": "0.0.3",
    "sass-loader": "6.0.x",
    "style-loader": "0.13.x",
    "tether": "1.3.x",
    "url-loader": "0.5.x",
    "vue": "1.0.x",
    "vue-hot-reload-api": "1.2.x",
    "vue-html-loader": "1.2.x",
    "vue-i18n": "4.0.x",
    "vue-loader": "8.5.x",
    "vue-router": "0.7.x",
    "vue-style-loader": "2.0.x",
    "vuex": "^1.0.0-rc.2",
    "webpack": "1.12.x",
    "webpack-dev-server": "1.12.x",
    "xo": "0.16.x"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "import/newline-after-import": 0,
      "camelcase": 0
    },
    "ignores": [
      "webpack.config.js",
      "server.js",
      "src/services/array.polyfill.js",
      "test/_helpers.js"
    ],
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "max-lines": 0
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": false
  },
    "engines": {
      "node": "^4.8.0 || ^5.7.0 || ^6.2.2 || >=8.0.0"
  }
}
